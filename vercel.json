{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/sync",
      "schedule": "03 20 * * *"
    },
    {
      "path": "/api/pluggy/webhook-worker",
      "schedule": "*/1 * * * *"
    }
  ]
}
